{
  "name": "trackenium",
  "scripts": {
    "start": "npm run webpack:watch:dev:localhost",
    "build:dev": "npm run clean:dist && npm run webpack:build:localhost",
    "build:prod": "npm run clean:dist && npm run compile:aot && npm run webpack:build:prod:localhost && npm run clean:aot",
    "build:prod:server": "npm run clean:dist && npm run compile:aot && npm run webpack:build:prod:server && npm run clean:aot",
    "compile:aot": "ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json",
    "webpack:watch:dev:localhost": "cross-env NODE_ENV=dev TARGET_MACHINE=localhost node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress",
    "webpack:build:dev:localhost": "cross-env NODE_ENV=dev TARGET_MACHINE=localhost node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile --bail",
    "webpack:build:prod:localhost": "cross-env NODE_ENV=prod TARGET_MACHINE=localhost node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile --bail",
    "webpack:build:prod:server": "cross-env NODE_ENV=prod TARGET_MACHINE=server node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile --bail",
    "clean:dist": "rimraf dist",
    "clean:aot": "rimraf src/**/*.js src/**/*.ngsummary.json",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:fix": "tslint ./src/**/*.ts --fix"
  },
  "dependencies": {
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "5.2.5",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "5.1.0",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "bowser": "1.9.2",
    "chart.js": "2.7.1",
    "core-js": "2.5.3",
    "decimal.js": "9.0.1",
    "flatpickr": "^4.3.2",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.5",
    "@ngtools/webpack": "1.9.8",
    "@types/core-js": "0.9.36",
    "@types/node": "7.0.7",
    "angular-router-loader": "0.8.2",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.4.1",
    "codelyzer": "4.1.0",
    "copy-webpack-plugin": "4.4.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "ngrx-store-freeze": "0.2.1",
    "node-sass": "4.7.2",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "2.1.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "to-string-loader": "1.1.5",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.6.0",
    "typescript": "2.6.2",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  }
}
